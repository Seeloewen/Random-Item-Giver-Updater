<Page x:Class="RandomItemGiverUpdater.Gui.Pages.ItemAdding.Page6_Finished"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:RandomItemGiverUpdater.Gui.Pages.ItemAdding"
      mc:Ignorable="d" 
      d:DesignHeight="580" d:DesignWidth="742"
      Title="Page1_Start">
    <Canvas x:Name="cvsContent">
        <Button x:Name="btnContinue" Content="Finish" HorizontalAlignment="Left" VerticalAlignment="Top" Height="25" Width="116" Canvas.Left="606" Canvas.Top="545" Click="btnContinue_Click"/>
        <Button x:Name="btnBack" Content="Back" HorizontalAlignment="Left" VerticalAlignment="Center" Height="25" Width="116" Canvas.Left="473" Canvas.Top="545" Click="btnBack_Click"/>
        <Label x:Name="lblHeader" Content="Step 6 - Finishing" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Foreground="White" FontSize="20" FontWeight="SemiBold"/>

        <TextBlock x:Name="tblAddedItems" Canvas.Left="17" TextWrapping="Wrap" Text="Your items have been added to the datapack! You can view a list of all added items down below. Click 'Finish' to exit the wizard." Canvas.Top="52" HorizontalAlignment="Left" VerticalAlignment="Top" Width="705" Foreground="White" FontSize="14"/>
        <TextBlock x:Name="tblElapsedTime" Canvas.Left="117" TextWrapping="Wrap" Text="Elapsed time: /" Canvas.Top="71" FontWeight="DemiBold" HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="14" Foreground="White"/>
        <TextBlock x:Name="tblAddedItemsList" Canvas.Left="19" TextWrapping="Wrap" Text="Added items:" Canvas.Top="122" FontWeight="DemiBold" FontSize="18" HorizontalAlignment="Left" VerticalAlignment="Top" Foreground="White" RenderTransformOrigin="0.596,0.444"/>
        <TextBox x:Name="tbAddedItemsList" Canvas.Left="19" TextWrapping="Wrap" Canvas.Top="161" Width="703" Height="373" Background="#FF636363" Foreground="White" IsReadOnly="True" HorizontalAlignment="Left" VerticalAlignment="Center"/>
    </Canvas>
</Page>
