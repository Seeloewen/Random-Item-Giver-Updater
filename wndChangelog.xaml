<Window x:Name="wndChangelog1" x:Class="Random_Item_Giver_Updater.wndChangelog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Random_Item_Giver_Updater"
        mc:Ignorable="d"
        Title="Changelog" Height="498" Width="596" Background="#FF323232" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <Canvas>
            <TextBlock x:Name="tblHeader" HorizontalAlignment="Left" Margin="18,10,0,0" TextWrapping="Wrap" Text="Changelog" VerticalAlignment="Top" FontSize="22" Foreground="White" FontWeight="SemiBold"/>
            <TextBox x:Name="tbChangelog" HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Center" Width="549" Height="345" Canvas.Left="18" Canvas.Top="52" FontSize="14" Text="Version Public Beta (03.08.2023)&#xA;* Initial Public Beta release featuring the Core UI with the ability to add, edit or remove items as well as view loot tables" Background="#FF444444" Foreground="White" IsReadOnly="True"/>
            <Button x:Name="btnOK" Content="OK" Canvas.Left="222" Canvas.Top="413" Height="29" Width="148" FontSize="15" Click="btnOK_Click" HorizontalAlignment="Left" VerticalAlignment="Center"/>
        </Canvas>
    </Grid>
</Window>
